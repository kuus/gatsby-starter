{
  "name": "@kuus/gatsby-starter",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "0.0.1",
  "author": "kuus",
  "license": "MIT",
  "scripts": {
    "_start": "npx concurrently 'npm:start:*'",
    "start": "npx gatsby develop",
    "start:gatsby": "npx gatsby develop",
    "start:cms": "npx netlify-cms-proxy-server",
    "cms": "npx netlify-cms-proxy-server",
    "build": "npx gatsby build",
    "clean": "npx gatsby clean",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "kill": "killall -9 node",
    "postinstall": "npm link @kuus/gatsby-plugin-i18n"
  },
  "devDependencies": {
    "@kuus/gatsby-plugin-i18n": "^0.6.1",
    "@mdx-js/mdx": "^1.x.x",
    "@mdx-js/react": "^1.x.x",
    "@types/styled-components": "^5.1.10",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "gatsby": "^3.7.1",
    "gatsby-cli": "^3.7.1",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-mdx": "^2.x.x",
    "gatsby-plugin-netlify": "^3.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-plugin-styled-components": "^4.7.1",
