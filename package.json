{
  "name": "@kuus/gatsby-starter",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "0.0.1",
  "author": "kuus",
  "license": "MIT",
  "scripts": {
    "_start": "npx concurrently 'npm:start:*'",
    "start": "npx gatsby develop",
    "start:gatsby": "npx gatsby develop",
    "start:cms": "npx netlify-cms-proxy-server",
    "cms": "npx netlify-cms-proxy-server",
    "build": "npx gatsby clean && npx gatsby build",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "kill": "killall -9 node"
  },
  "dependencies": {
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@kuus/gatsby-plugin-i18n": "0.0.9",
    "@mdx-js/mdx": "^2.0.0-next.8",
    "@mdx-js/react": "^2.0.0-next.8",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^6.0.0",
    "gatsby": "^3.0.1",
    "gatsby-cli": "^3.0.0",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-mdx": "^2.0.0",
    "gatsby-plugin-netlify": "^3.0.0",
    "gatsby-plugin-netlify-cms": "^5.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-plugin-styled-components": "^4.0.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^3.0.0",
    "gatsby-remark-autolink-headers": "^3.0.0",
    "gatsby-remark-copy-linked-files": "^3.0.0",
    "gatsby-remark-image-attributes": "^0.2.8",
    "gatsby-remark-images": "^4.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-remark": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "hast-util-from-dom": "^3.0.0",
    "jsdom": "^16.4.0",
    "nanoid": "^3.1.20",
    "netlify-cms-app": "^2.14.26",
    "netlify-cms-proxy-server": "^1.3.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remark-image-attributes": "^0.2.1",
    "styled-components": "^5.2.1",
    "unist-util-visit": "^2.0.3"
  }
}
